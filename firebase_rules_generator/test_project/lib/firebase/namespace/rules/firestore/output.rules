rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    function test() {
      let a = exists(path('/databases/$(database)/documents/path/to/resource'));
      let b = existsAfter(path('/databases/$(database)/documents/path/to/resource'));
      let c = get(path('/databases/$(database)/documents/path/to/resource'));
      let d = get(path('/databases/(default)/documents/path/to/resource')).asdf;
      let e = getAfter(path('/databases/$(database)/documents/path/to/resource'));
      let f = getAfter(path('/databases/$(database)/documents/path/to/resource')).asdf;
      return true;
    }
  }
}

rules_version = '2';
service firebase.storage {
  match /databases/{database}/documents {
    function test() {
      let a = firestore.exists(path('/databases/$(database)/documents/path/to/resource'));
      let b = firestore.existsAfter(path('/databases/$(database)/documents/path/to/resource'));
      let c = firestore.get(path('/databases/$(database)/documents/path/to/resource'));
      let d = firestore.get(path('/databases/(default)/documents/path/to/resource')).asdf;
      let e = firestore.getAfter(path('/databases/$(database)/documents/path/to/resource'));
      let f = firestore.getAfter(path('/databases/$(database)/documents/path/to/resource')).asdf;
      return true;
    }
  }
}
